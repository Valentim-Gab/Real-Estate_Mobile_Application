generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int      @id @default(autoincrement())
  name     String   @db.VarChar(100)
  email    String   @unique @db.VarChar(250)
  password String   @db.VarChar(200)
  role     String[] @db.VarChar(10)
}
